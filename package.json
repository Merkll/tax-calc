{
  "name": "server-support",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn db:rollback && tsc",
    "start:db": "docker-compose -f .docker/docker-compose.yaml up -d db",
    "stop:db": "docker-compose -f .docker/docker-compose.yaml down",
    "start:test:db": "docker-compose -f .docker/docker-compose.yaml up -d testdb",
    "stop:test:db": "docker-compose -f .docker/docker-compose.yaml down",
    "start": "node ./build/src/app.js",
    "dev": "yarn start:db && nodemon",
    "prod": "node ./build.js",
    "db:migrate": "yarn sequelize-cli:ts db:migrate",
    "db:rollback": "yarn db:undo && yarn db:migrate",
    "db:seed": "yarn sequelize-cli:ts db:seed:all",
    "db:undo": "yarn sequelize-cli:ts db:migrate:undo:all",
    "generate:doc": "yarn build && swagger-jsdoc -d build/src/config/swagger.js  -o ./docs/swagger.json && rm -rf ./build",
    "sequelize-cli:ts": "./sequelize.sh $*"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.7",
    "bcrypt": "^4.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.4.1",
    "morgan": "^1.9.1",
    "pg": "^7.17.0",
    "sequelize": "^5.21.3",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/mongodb": "^3.3.14",
    "@types/mongoose": "^5.5.34",
    "@types/node": "^13.1.6",
    "@types/pg": "^7.14.1",
    "@types/redis": "^2.8.14",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "fs-extra": "^8.1.0",
    "nodemon": "*",
    "sequelize-cli": "^5.5.1",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
